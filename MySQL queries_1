SELECT nombre FROM producto;
SELECT nombre, precio FROM producto;
SHOW COLUMNS FROM producto;
SELECT nombre, precio,  precio*0.90 FROM producto;
SELECT nombre AS 'nombre de producto', precio AS euros, precio*0.90 AS 'dolares americanos' FROM producto; 
SELECT UPPER(nombre), precio AS euros, precio*0.90 AS 'dolares americanos' FROM producto; 
SELECT lower(nombre), precio AS euros, precio*0.90 AS 'dolares americanos' FROM producto;
SELECT nombre, UPPER(CONCAT (LEFT(nombre,2))) AS Upper FROM fabricante;
SELECT nombre, ROUND ((precio), 2) AS precio FROM producto;
SELECT nombre, TRUNCATE((precio), 0) AS precio FROM producto;
SELECT codigo_fabricante FROM producto;
SELECT DISTINCT codigo_fabricante FROM producto;
SELECT nombre FROM fabricante ORDER BY nombre ASC;
SELECT nombre FROM fabricante ORDER BY nombre DESC;
SELECT nombre AS NOM_ASC, nombre AS NOM_PREUDESC FROM producto ORDER BY nombre ASC, precio DESC;
SELECT * FROM fabricante limit 0,5;
SELECT * FROM fabricante limit 3,2;
SELECT nombre, precio FROM producto order by precio limit 0,1;
SELECT nombre, precio FROM producto order by precio desc limit 0,1;
SELECT nombre FROM producto where codigo_fabricante=2;
SELECT p.nombre AS nomProducto, p.precio, f.nombre AS nomFabricante from producto p, fabricante f WHERE p.codigo_fabricante=f.codigo;
SELECT p.nombre AS nomProducto, p.precio, f.nombre AS nomFabricante from producto p, fabricante f WHERE p.codigo_fabricante=f.codigo ORDER BY f.nombre;
SELECT p.codigo AS codigoProducto, p.nombre AS nomProducto, f.codigo AS codigoFabricante, f.nombre AS NomFabricante from producto p, fabricante f WHERE p.codigo_fabricante=f.codigo ORDER BY f.nombre;
SELECT p.nombre AS nomProducto, p.precio, f.nombre AS nomFabricante from producto p, fabricante f WHERE p.codigo_fabricante=f.codigo ORDER BY precio limit 0,1;
SELECT p.nombre AS nomProducto, p.precio, f.nombre AS nomFabricante from producto p, fabricante f WHERE p.codigo_fabricante=f.codigo ORDER BY precio desc limit 0,1;
SELECT p.codigo AS codigoProducto, p.nombre AS nomProducto, p.precio, f.nombre AS nomFabricante, f.codigo AS codigoFabricante from producto p, fabricante f WHERE p.codigo_fabricante=f.codigo AND f.nombre='Lenovo';
SELECT p.codigo AS codigoProducto, p.nombre AS nomProducto, p.precio, f.nombre AS nomFabricante, f.codigo AS codigoFabricante from producto p, fabricante f WHERE p.codigo_fabricante=f.codigo AND f.nombre='Crucial' AND p.precio>200;
SELECT p.codigo AS codigoProducto, p.nombre AS nomProducto, p.precio, f.nombre AS nomFabricante FROM producto p, fabricante f WHERE p.codigo_fabricante=f.codigo AND (f.nombre='Asus' OR f.nombre='Hewlett-Packard'OR f.nombre='Seagate');
SELECT p.codigo AS codigoProducto, p.nombre AS nomProducto, p.precio, f.nombre AS nomFabricante FROM producto p, fabricante f WHERE p.codigo_fabricante=f.codigo AND f.nombre IN ('Asus', 'Hewlett-Packard', 'Seagate');
SELECT p.nombre AS nomProducto, p.precio FROM producto p, fabricante f WHERE p.codigo_fabricante=f.codigo AND f.nombre LIKE '%e';
SELECT p.nombre AS nomProducto, p.precio FROM producto p, fabricante f WHERE p.codigo_fabricante=f.codigo AND f.nombre LIKE '%w%';
SELECT p.nombre AS nomProducto, p.precio, f.nombre AS nomFabricante from producto p, fabricante f WHERE p.codigo_fabricante=f.codigo AND p.precio>=180 ORDER BY p.precio DESC, p.nombre ASC;
SELECT DISTINCT f.codigo, f.nombre AS nomFabricante FROM producto p, fabricante f WHERE p.codigo_fabricante=f.codigo;
SELECT fabricante.nombre AS nomFabricante, producto.nombre AS nomProducto from fabricante LEFT JOIN producto ON fabricante.codigo=producto.codigo_fabricante;
SELECT fabricante.nombre FROM fabricante LEFT JOIN producto ON fabricante.codigo!=producto.codigo_fabricante WHERE NOT fabricante.codigo = ANY (SELECT producto.codigo_fabricante FROM producto) LIMIT 10,2;
SELECT p.nombre AS nomProducto, f.nombre AS nomFabricante FROM producto p, fabricante f WHERE p.codigo_fabricante=f.codigo AND f.nombre LIKE '%lenovo%';
SELECT p.codigo, p.nombre AS nomProducto, p.precio, f.nombre AS nomFabricante, f.codigo from producto p, fabricante f WHERE p.codigo_fabricante=f.codigo AND p.precio =(SELECT MAX(p.precio) FROM producto p, fabricante f WHERE p.codigo_fabricante=f.codigo AND f.nombre LIKE '%Lenovo%'); 
SELECT p.nombre AS nomProducto FROM producto p, fabricante f WHERE p.codigo_fabricante=f.codigo AND p.precio =(SELECT MAX(p.precio) FROM producto p, fabricante f WHERE p.codigo_fabricante=f.codigo AND f.nombre LIKE '%Lenovo%');
SELECT p.nombre AS nomProducto FROM producto p, fabricante f WHERE p.codigo_fabricante=f.codigo AND p.precio =(SELECT MIN(p.precio) FROM producto p, fabricante f WHERE p.codigo_fabricante=f.codigo AND f.nombre LIKE '%Hewlett-Packard%');
SELECT * FROM producto p WHERE p.precio >= ( SELECT MAX(p.precio) FROM fabricante f INNER JOIN producto p ON f.codigo = p.codigo_fabricante WHERE f.nombre = 'Lenovo');
SELECT * FROM producto p, fabricante f WHERE p.codigo_fabricante=f.codigo AND f.nombre LIKE '%Asus%' AND p.precio > (SELECT AVG (precio) FROM producto p, fabricante f WHERE p.codigo_fabricante=f.codigo AND f.nombre LIKE '%Asus%');
